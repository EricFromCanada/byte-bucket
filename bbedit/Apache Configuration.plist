		<key>Number Pattern</key>
		<string></string>
		<key>String Pattern</key>
		<string>(?sx:
			"[^"\\]*(?:\\.[^"\\]*)*"	(?# double quote)	|
			'[^'\\]*(?:\\.[^'\\]*)*'	(?# single quote)
		)</string>
		<key>Skip Pattern</key>
		<string>(?x:
			(?P&gt;comment)	|
			(?P&gt;string)
		)</string>
